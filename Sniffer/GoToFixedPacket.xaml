<Window x:Class="Sniffer.GoToFixedPacket"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sniffer: Go To Packet" Height="100" Width="200">
    <Grid>
    	<Label Content="Packet Number" FontSize="10" HorizontalAlignment="Left" Margin="6,6,0,0" Width="83" Height="23" VerticalAlignment="Top"/>
    	<TextBox x:Name="PacketNumber" HorizontalAlignment="Right" Margin="0,4,21,0" TextWrapping="Wrap" Width="70" Height="23" VerticalAlignment="Top"/>
    	<Button Content="Jump to" HorizontalAlignment="Left" Height="21" Margin="12,0,0,8" VerticalAlignment="Bottom" Width="63" Click="Button_Click_1" />
        <Button Content="Cancel" Height="21" Margin="0,0,28,8" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="63" Click="Button_Click" />
    </Grid>
</Window>
